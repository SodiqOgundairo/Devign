{
  "buildCommand": "npm ci --prefix playground && npm run build --prefix playground",
  "outputDirectory": "playground/dist",
  "framework": "vite",
  "installCommand": "npm ci && npm ci --prefix playground",
  "functions": {
    "api/**/*.ts": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "env": {
    "VITE_API_URL": "@vite_api_url"
  },
  "public": true
}
